# Smart Agriculture Mobile App Environment Variables

# WebSocket Server URL (ESP32 WebSocket endpoint)
EXPO_PUBLIC_WS_URL=ws://192.0.0.2:8080/ws

# FastAPI Backend URL (Machine Learning and Analytics API)
EXPO_PUBLIC_API_URL=http://localhost:8000

# OpenWeather API Key (for weather forecasts)
EXPO_PUBLIC_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_MOCK_DATA=false